#include <iostream>
#include <vector>
using namespace std;

class MaxPQ {
    vector<int> h;

    void heapifyUp(int i) {
        while (i > 0 && h[(i - 1) / 2] < h[i]) {
            swap(h[i], h[(i - 1) / 2]);
            i = (i - 1) / 2;
        }
    }

    void heapifyDown(int i) {
        int n = h.size();
        while (true) {
            int left = 2 * i + 1, right = 2 * i + 2;
            int largest = i;
            if (left < n && h[left] > h[largest]) largest = left;
            if (right < n && h[right] > h[largest]) largest = right;
            if (largest == i) break;
            swap(h[i], h[largest]);
            i = largest;
        }
    }

public:
    void push(int x) {
        h.push_back(x);
        heapifyUp(h.size() - 1);
    }

    void pop() {
        if (h.empty()) return;
        h[0] = h.back();
        h.pop_back();
        if (!h.empty()) heapifyDown(0);
    }

    int top() { return h.empty() ? -1 : h[0]; }

    bool empty() { return h.empty(); }

    int size() { return h.size(); }
};


class MinPQ {
    vector<int> h;

    void heapifyUp(int i) {
        while (i > 0 && h[(i - 1) / 2] > h[i]) {
            swap(h[i], h[(i - 1) / 2]);
            i = (i - 1) / 2;
        }
    }

    void heapifyDown(int i) {
        int n = h.size();
        while (true) {
            int left = 2 * i + 1, right = 2 * i + 2;
            int smallest = i;
            if (left < n && h[left] < h[smallest]) smallest = left;
            if (right < n && h[right] < h[smallest]) smallest = right;
            if (smallest == i) break;
            swap(h[i], h[smallest]);
            i = smallest;
        }
    }

public:
    void push(int x) {
        h.push_back(x);
        heapifyUp(h.size() - 1);
    }

    void pop() {
        if (h.empty()) return;
        h[0] = h.back();
        h.pop_back();
        if (!h.empty()) heapifyDown(0);
    }

    int top() { return h.empty() ? -1 : h[0]; }

    bool empty() { return h.empty
